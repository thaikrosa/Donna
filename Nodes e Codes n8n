// Filtrar apenas os campos necessÃ¡rios
const usuarios = $input.all().map(item => ({
  id_user: item.json.id_user,
  nome: item.json.nome, 
  cidade: item.json.cidade,
  preferencias: item.json.preferencias
}));

return usuarios.map(user => ({ json: user }));

